{
    "require": {
        "php": "*",
        "ext-gettext": "*",
        "ext-hash": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "phpstan/phpstan": "^1.10"
    },
    "minimum-stability": "stable",
    "extra": {
        "installer-paths": {
            "dolibarr/packages/{$name}/": [
